{
  "name": "refinery-tools",
  "version": "0.4.7",
  "description": "Set of dev tools to inspect a refinery app.",
  "repository": "https://github.com/Platane/refinery-tools",
  "files": [
    "lib/"
  ],
  "main": "lib/index.js",
  "dependencies": {
    "klayjs": "^0.4.1"
  },
  "peerDependencies": {
    "refinery-js": "^0.4.2",
    "react": "^15.1.0"
  },
  "devDependencies": {
    "refinery-js": "^0.4.2",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-motion": "0.4.x",
    "expect": "1.x",
    "mocha": "2.5",
    "webpack": "1.x",
    "webpack-dev-server": "*",
    "file-loader": "0.9.x",
    "style-loader": "0.13.x",
    "css-loader": "0.23.x",
    "html-loader": "0.4.x",
    "babel-loader": "6.x",
    "babel-register": "6.x",
    "babel-polyfill": "6.x",
    "babel-core": "6.x",
    "babel-cli": "6.x",
    "babel-preset-es2015": "6.x",
    "babel-preset-react": "6.x",
    "babel-preset-stage-1": "6.x"
  },
  "scripts": {
    "start": "node script/server-examples.js",
    "build:examples": "rm -rf dist && node script/build-examples.js",
    "build:lib": "rm -rf lib && babel src/web_modules --out-dir lib && node script/build-style.js",
    "prepublish": "npm run build:lib",
    "test": "env NODE_ENV=\"test\" mocha --compilers js:babel-register --recursive  --timeout 200",
    "test:watch": "npm test -- --watch --bail"
  }
}
