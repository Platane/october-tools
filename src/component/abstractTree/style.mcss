.root,
.node{

}
.name{
    display: flex;
    flex-direction: row;
    align-items: center;
    cursor: pointer;
    display: inline-flex;
    padding-right: 6px;
}
.children{
    margin-left: 20px;
}
.value{
    margin-left: auto;
    user-select: text;
}

.ticNodeClose,
.ticNodeOpen{
    width: 20px;
    text-align: center;
    color: #ddd;
    transition: transform 120ms ease;
}
.ticNodeClose{
    transform: rotate(-90deg);
}
