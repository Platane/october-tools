.container{
    font-family: 'Helvetica Neue', Helvetica, Arial, sans-serif;
    font-size:14px;

    display: flex;
    flex-direction: row;
    user-select: none;
}

.actionList{
    margin: 10px;
    padding: 10px;
    width: calc( 40% - 21px );
    border-left: solid 1px #ddd;
}
.stateTree{
    margin: 10px;
    padding: 10px;
    width: calc( 60% - 20px );
}
